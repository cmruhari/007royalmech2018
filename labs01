>>> h=[1,2,3,4,5]
>>> h.append(6)
>>> 
>>> h
[1, 2, 3, 4, 5, 6]
>>> h.append([6,7])
>>> h
[1, 2, 3, 4, 5, 6, [6, 7]]
>>> h.append(6)
>>> h
[1, 2, 3, 4, 5, 6, [6, 7], 6]
>>> h.count(6)
2
>>> h.index(2)
1
>>> h.index(3)
>>> h.remove(2)
>>> h
[1, 3, 4, 5, 6, [6, 7], 6]
>>> 




